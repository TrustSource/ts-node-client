{
  "name": "ts-node-client",
  "packageManager": "yarn@3.5.0",
  "description": "npm / node module to transfer dependency information to TrustSource server.",
  "version": "3.1.0",
  "homepage": "https://app.trustsource.io/",
  "author": {
    "name": "Gerhard Dickescheid",
    "email": "gdd@eacg.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TrustSource/ts-node-client.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "./lib/cli.js",
  "bin": "./bin/ts-node-client.js",
  "scripts": {
    "scan": "yarn node ./bin/ts-node-client.js --debug true --saveAs test --saveAsFormat scan",
    "scan-with-brakes": "yarn node ./bin/ts-node-client.js --breakOnViolations true --breakOnWarnings false",
    "scan-to-file": "yarn node ./bin/ts-node-client.js --saveAs test --saveAsFormat scan",
    "lint": "eslint bin lib test",
    "lint-fix": "eslint bin lib test --fix",
    "test": "mocha test",
    "precommit": "npm run lint && npm run test"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "axios": "1.2.6",
    "debuglog": "1.0.1",
    "js-yaml": "^4.1.0",
    "semver": "7.3.8",
    "yargs": "17.7.1"
  },
  "devDependencies": {
    "eslint": "8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sonarjs": "0.18.0",
    "mocha": "10.2.0",
    "nock": "13.3.0"
  },
  "keywords": [
    "node",
    "npm",
    "meteor",
    "plugin",
    "opensource",
    "cydx",
    "spdx",
    "dependency-analysis"
  ]
}
