{
  "name": "ts-node-client",
  "description": "npm / node module to transfer dependency information to TrustSource server.",
  "version": "2.1.1",
  "private": false,
  "homepage": "https://app.trustsource.io/",
  "author": {
    "name": "Gerhard Dickescheid",
    "email": "gdd@eacg.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TrustSource/ts-node-client.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 8.9.0"
  },
  "main": "./lib/cli.js",
  "bin": {
    "ts-node-client": "./bin/ts-node-client.js"
  },
  "scripts": {
    "scan": "node ./bin/ts-node-client.js",
    "scan-with-brakes": "node ./bin/ts-node-client.js --breakOnViolations true --breakOnWarnings false",
    "scan-to-file": "node ./bin/ts-node-client.js --saveAs test --saveAsFormat cydx",
    "lint": "eslint bin lib test",
    "lint-fix": "eslint bin lib test --fix",
    "test": "mocha test",
    "precommit": "npm run lint && npm run test"
  },
  "dependencies": {
    "npm": "6.14.17",
    "axios": "0.26.1",
    "debuglog": "1.0.1",
    "semver": "7.3.5",
    "yargs": "^17.5.0"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "mocha": "^9.2.2",
    "nock": "^12.0.3"
  },
  "keywords": [
    "node",
    "npm",
    "meteor",
    "plugin",
    "opensource",
    "cydx",
    "spdx",
    "dependency-analysis"
  ]
}
